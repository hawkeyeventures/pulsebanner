{
    "version": 2,
    "projects": {
        "frontend": "apps/frontend",
        "frontend-e2e": "apps/frontend-e2e",
        "remotion": "apps/remotion",
        "templates": "libs/templates"
    },
    "cli": {
        "defaultCollection": "@nrwl/nest"
    },
    "generators": {
        "@nrwl/react": {
            "application": {
