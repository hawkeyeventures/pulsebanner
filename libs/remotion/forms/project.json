{
    "root": "libs/remotion/forms",
    "sourceRoot": "libs/remotion/forms/src",
    "projectType": "library",
    "tags": [],
    "targets": {
        "lint": {
            "executor": "@nrwl/linter:eslint",
            "outputs": [
                "{options.outputFile}"
            ],
            "options": {
                "lintFilePatterns": [
                    "libs/remotion/forms/**/*.{ts,tsx,js,jsx}"
                ]
            }
        },
        "test": {
            "executor": "@nrwl/jest:jest",
            "outputs": [
                "coverage/libs/remotion/forms"
            ],
            "options": {
                "jestConfig": "libs/remotion/forms/jest.config.js",
                "passWithNoTests": true
            }
        }
    }
}
